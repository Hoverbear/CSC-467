\begin_layout Section
Delays Experienced by Flows
\end_layout

\begin_layout Subsection
Packet Service Time for Class 1
\end_layout

\begin_layout Standard
\begin_inset Formula $\mu_{1}=\frac{1Mb}{500b}=2000$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{\mu_{1}}=\frac{1}{2000}s
\]

\end_inset


\end_layout

\begin_layout Subsection
Packet Service Time for Class 2
\end_layout

\begin_layout Standard
\begin_inset Formula $\mu_{1}=\frac{1Mb}{1000b}=1000$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{\mu_{2}}=\frac{1}{1000}s
\]

\end_inset


\end_layout

\begin_layout Subsection
Class 1 at 25, Class 2 at 50
\end_layout

\begin_layout Standard
Setting 
\begin_inset Formula $\rho_{1}=\frac{\lambda_{1}}{\mu_{1}}=\frac{25}{2000}$
\end_inset

,
\begin_inset Formula $\rho_{2}=\frac{50}{1000}$
\end_inset

.
 
\begin_inset Formula $E\{R\}=\frac{25(\frac{2}{2000^{2}})+50(\frac{2}{1000^{2}})}{2}=0.00005625$
\end_inset

.
\end_layout

\begin_layout Standard
Expected wait times:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E(\{w_{1}\}=\frac{E(R)}{1-\frac{25}{2000}}=0.00005696s
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E\{w_{2}\}=\frac{E(R)}{1-\frac{50}{1000}}=0.00005921s
\]

\end_inset


\end_layout

\begin_layout Standard
Expected number in queue:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E\{n_{1}\}=\lambda_{1}E\{w_{1}\}=25*0.00005696=0.001424
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E\{n_{2}\}=\lambda_{2}E\{w_{2}\}=50*0.00005921=0.002961
\]

\end_inset


\end_layout

\begin_layout Standard
Utilization:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\rho_{1}+\rho_{2}=\frac{125}{2000}=\frac{1}{16}
\]

\end_inset


\end_layout

\begin_layout Subsection
Class 1 at 50, Class 2 at 50
\end_layout

\begin_layout Standard
Setting 
\begin_inset Formula $\rho_{1}=\frac{\lambda_{1}}{\mu_{1}}=\frac{50}{2000}$
\end_inset

,
\begin_inset Formula $\rho_{2}=\frac{50}{1000}$
\end_inset

.
 
\begin_inset Formula $E\{R\}=\frac{50(\frac{2}{2000^{2}})+50(\frac{2}{1000^{2}})}{2}=0.0000625$
